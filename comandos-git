git log # Mostra o historico dos commits
git log --oneline # Mostra os historicos de forma resumida
git log --graph # Mostra um grafico com os branches
git lot --oneline --graph
git check out -b branch_name # Cria um novo branch e entra novo branch
git check out branch_name    # move to the branch_name:w
                            # Os arquivos tb sao alterados para o status da ramificacao

git status # Mostra as modificacoes no arquivo
git add . # Adiciona todos os arquivos para commit
git commit -m "message" # Commit dos arquivos adicionados 

git merge branch_name # Eh preciso esta no branch master.
git remote  # Verifica se tem um repositorio remoto
git remote --v  # Mais detalhes do repositorio remoto


# Criando um repositorio no remoto no github e upload o projeto local para o git
git push -u origin master  # enviar repsitorio local para o github

git clone <git-directory>  # clone a repository 

git fetch # Semelhante ao pull, mas nao faz o merge 
git checkout origin # Entra no repositorio remoto
git checkout master # Entra no Master
git pull  # atualiza o repositorio remote para local (master)
